language ESSENCE' 1.0

find x_ExplicitVarSizeWithMarker_Marker: int(0..3)
find x_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..3)] of int(1..3)
find x_ExplicitVarSizeWithDummy: matrix indexed by [int(1..3)] of int(1..4)
find cut1: bool
find cut2: bool
branching on
    [cut1, cut2, x_ExplicitVarSizeWithDummy, x_ExplicitVarSizeWithMarker_Marker, x_ExplicitVarSizeWithMarker_Values]
such that
    !cut1 <->
    2 <= x_ExplicitVarSizeWithMarker_Marker /\
    (or([q24 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q24] = 1 | q24 : int(1..3)]) /\
     or([q26 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q26] = 2 | q26 : int(1..3)])),
    !cut2 <-> or([x_ExplicitVarSizeWithDummy[q28] != 4 /\ x_ExplicitVarSizeWithDummy[q28] = 1 | q28 : int(1..3)]),
    and([q1 + 1 <= x_ExplicitVarSizeWithMarker_Marker ->
         x_ExplicitVarSizeWithMarker_Values[q1] < x_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..2)]),
    and([q2 > x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[q2] = 1 | q2 : int(1..3)]),
    and([x_ExplicitVarSizeWithDummy[q4] < x_ExplicitVarSizeWithDummy[q4 + 1] \/ x_ExplicitVarSizeWithDummy[q4] = 4
             | q4 : int(1..2)]),
    and([x_ExplicitVarSizeWithDummy[q5] = 4 -> x_ExplicitVarSizeWithDummy[q5 + 1] = 4 | q5 : int(1..2)]),
    sum([toInt(x_ExplicitVarSizeWithDummy[q9] != 4) | q9 : int(1..3)]) = x_ExplicitVarSizeWithMarker_Marker,
    sum([toInt(x_ExplicitVarSizeWithDummy[q15] != 4) | q15 : int(1..3)]) <= x_ExplicitVarSizeWithMarker_Marker,
    and([x_ExplicitVarSizeWithDummy[q11] != 4 ->
         or([q13 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q13] = x_ExplicitVarSizeWithDummy[q11]
                 | q13 : int(1..3)])
             | q11 : int(1..3)]),
    x_ExplicitVarSizeWithMarker_Marker <= sum([toInt(x_ExplicitVarSizeWithDummy[q18] != 4) | q18 : int(1..3)]),
    and([q19 <= x_ExplicitVarSizeWithMarker_Marker ->
         or([x_ExplicitVarSizeWithDummy[q21] != 4 /\
             x_ExplicitVarSizeWithDummy[q21] = x_ExplicitVarSizeWithMarker_Values[q19]
                 | q21 : int(1..3)])
             | q19 : int(1..3)])

