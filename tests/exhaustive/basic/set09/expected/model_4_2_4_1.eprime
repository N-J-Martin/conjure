language ESSENCE' 1.0

find x_ExplicitVarSizeWithFlags_Flags: matrix indexed by [int(1..4)] of bool
find x_ExplicitVarSizeWithFlags_Values: matrix indexed by [int(1..4)] of int(2..5)
find y_ExplicitVarSizeWithDummy: matrix indexed by [int(1..4)] of int(2..6)
find y_Occurrence: matrix indexed by [int(2..5)] of bool
branching on
    [x_ExplicitVarSizeWithFlags_Flags, x_ExplicitVarSizeWithFlags_Values, y_Occurrence, y_ExplicitVarSizeWithDummy]
such that
    and([x_ExplicitVarSizeWithFlags_Flags[q25] /\ y_ExplicitVarSizeWithDummy[q26] != 6 ->
         x_ExplicitVarSizeWithFlags_Values[q25] + 2 = y_ExplicitVarSizeWithDummy[q26]
             | q25 : int(1..4), q26 : int(1..4)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q1 + 1] ->
