    x_ExplicitVarSizeWithDummy[1] = 3 -> x_ExplicitVarSizeWithDummy[2] = 3,
    sum([toInt(x_ExplicitVarSizeWithDummy[q4] != 3) | q4 : int(1..2)]) <= 2,
    sum([toInt(x_ExplicitVarSizeWithDummy[q7] != 3) | q7 : int(1..2)]) =
