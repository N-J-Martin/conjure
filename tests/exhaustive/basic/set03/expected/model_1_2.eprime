    sum([toInt(x_ExplicitVarSizeWithDummy[q12] != 3) | q12 : int(1..2)]) <=
    sum([toInt(x_Occurrence[q13]) | q13 : int(1..2)]),
    and([x_ExplicitVarSizeWithDummy[q10] != 3 -> x_Occurrence[x_ExplicitVarSizeWithDummy[q10]] | q10 : int(1..2)]),
    sum([toInt(x_Occurrence[q17]) | q17 : int(1..2)]) <=
    sum([toInt(x_ExplicitVarSizeWithDummy[q19] != 3) | q19 : int(1..2)]),
    and([x_Occurrence[q14] ->
         or([x_ExplicitVarSizeWithDummy[q16] != 3 /\ x_ExplicitVarSizeWithDummy[q16] = q14 | q16 : int(1..2)])
             | q14 : int(1..2)])

