    sum([toInt(x_ExplicitVarSizeWithDummy[q16] != 3) | q16 : int(1..2)]),
    and([x_Occurrence[q11] ->
         or([x_ExplicitVarSizeWithDummy[q13] != 3 /\ x_ExplicitVarSizeWithDummy[q13] = q11 | q13 : int(1..2)])
             | q11 : int(1..2)]),
