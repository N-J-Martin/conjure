    sum([toInt(x_Occurrence[q21]) | q21 : int(1..2)]),
    and([x_ExplicitVarSizeWithDummy[q18] != 3 -> x_Occurrence[x_ExplicitVarSizeWithDummy[q18]] | q18 : int(1..2)])

