    x_ExplicitVarSizeWithDummy[1] != 2 /\ y_Occurrence[x_ExplicitVarSizeWithDummy[1]] -> false,
    0 <= toInt(x_ExplicitVarSizeWithDummy[1] != 2 /\ y_Occurrence[x_ExplicitVarSizeWithDummy[1]])

