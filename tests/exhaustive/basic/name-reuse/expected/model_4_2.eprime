    sum([toInt(s_ExplicitVarSizeWithDummy[q19] != 5) | q19 : int(1..4)]) <=
    sum([toInt(s_ExplicitVarSizeWithFlags_Flags[q21]) | q21 : int(1..4)]),
    and([s_ExplicitVarSizeWithDummy[q15] != 5 ->
         or([s_ExplicitVarSizeWithFlags_Flags[q17] /\
